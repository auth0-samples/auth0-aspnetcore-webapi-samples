{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "WebAPIApplication": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "api/public",
      "applicationUrl": "http://localhost:3010",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/api/public",
      "environmentVariables": {},
      "publishAllPorts": true
    }
  }
}